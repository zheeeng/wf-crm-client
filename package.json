  "scripts": {
    "preinstall": "unlink node_modules/~src 2> /dev/null || true",
    "postinstall": "ln -s `pwd`/src node_modules/~src",
    "start": "react-app-rewired start",
    "build:preview": "react-app-rewired build",
    "prebuild": "sentry-cli releases new $SENTRY_PROJECT@$SOURCE_VERSION",
    "build": "REACT_APP_VERSION=$SOURCE_VERSION react-app-rewired build",
    "postbuild": "react-snap && sentry-cli releases files $SENTRY_PROJECT@$SOURCE_VERSION upload-sourcemaps build --no-rewrite && find build -name *.js.map -exec rm {} \\; && sentry-cli releases finalize $SENTRY_PROJECT@$SOURCE_VERSION ",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint:fix": "eslint --fix --ext .ts,.tsx src/",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "heroku-postbuild": "NODE_ENV=production yarn build"
  },
  "devDependencies": {
    "@sentry/cli": "^1.47.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "customize-cra": "^0.5.0",
    "eslint": "^6.2.2",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-react-hooks": "^2.0.1",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "^3.1.1",
    "react-snap": "^1.23.0",
    "typescript": "^3.5.3"
  },
  "proxy": "https://crm-api.dev-waiverforeverk8s.com/",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "reactSnap": {
    "skipThirdPartyRequests": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  }
}
