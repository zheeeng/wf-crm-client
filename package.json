    "@types/debounce": "^1.2.0",
    "@types/jest": "^24.0.18",
    "@types/js-cookie": "^2.2.2",
    "@types/md5": "^2.1.33",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-select": "^3.0.4",
    "array-move": "^2.1.0",
