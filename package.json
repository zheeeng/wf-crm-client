  "scripts": {
    "preinstall": "unlink node_modules/~src 2> /dev/null || true",
    "postinstall": "ln -s `pwd`/src node_modules/~src",
    "start": "react-app-rewired start",
    "build:preview": "react-app-rewired build",
    "prebuild": "sentry-cli releases new $SENTRY_PROJECT@$SOURCE_VERSION",
    "build": "REACT_APP_VERSION=$SOURCE_VERSION react-app-rewired build",
    "build:local": "react-app-rewired build",
    "postbuild": "react-snap && sentry-cli releases files $SENTRY_PROJECT@$SOURCE_VERSION upload-sourcemaps build --no-rewrite && find build -name *.js.map -exec rm {} \\; && sentry-cli releases finalize $SENTRY_PROJECT@$SOURCE_VERSION ",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint:fix": "eslint --fix --ext .ts,.tsx src/",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "heroku-postbuild": "NODE_ENV=production yarn build"
  },
  "devDependencies": {
    "@sentry/cli": "^1.64.1",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "customize-cra": "^1.0.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
