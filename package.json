    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.0",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "4.0.3",
    "react-snap": "^1.23.0",
    "typescript": "^4.2.4"
  },
  "proxy": "https://crm-api.dev-waiverforeverk8s.com/",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "reactSnap": {
    "skipThirdPartyRequests": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  }
}
